<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="in.mcoeproject.copsos"
    tools:ignore="LockedOrientationActivity">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ComplaintRecieved"
            android:screenOrientation="sensorPortrait"
            ></activity>
        <activity android:name=".ComplaintCompleted"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".ComplaintReported"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".ComplaintCancelled"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".Otp"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".ComplaintForwarded"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".ManualComplaint"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".MainActivity"
            android:screenOrientation="sensorPortrait"
            />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".Status"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".Profile_Activity"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".Register"
            android:screenOrientation="sensorPortrait"
            />
        <activity android:name=".Splash"
            android:screenOrientation="sensorPortrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>